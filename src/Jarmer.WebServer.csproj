<Project Sdk="Microsoft.NET.Sdk">

<PropertyGroup>
		<Version>0.1.0</Version>
		<Authors>Ruben Labruyere</Authors>
		<Company>Ruben Labruyere</Company>
    <PackageId>jarmer-webserver</PackageId>
		<Description>
      Jarmer WebServer is an easy to use light-weight WebServer in dotnet built on top of AdvancedSockets
		</Description>
		<RepositoryUrl>https://github.com/TheBoneJarmer/jarmer-web-server.git</RepositoryUrl>
		<PackageTags>jarmer,webserver,web-server,web,advancedsockets</PackageTags>
		<AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
	</PropertyGroup>

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AdvancedSockets" Version="1.1.4" />
  </ItemGroup>

</Project>
